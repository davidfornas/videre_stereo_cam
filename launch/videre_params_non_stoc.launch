<launch>
    <group ns="stereo">
        <node name="videre_stereo_cam" pkg="videre_stereo_cam" type="videre_stereo_node" respawn="true" output="screen">
            <param name="fps" type="double" value="30"/>
            <param name="frame_id" value="stereo_optical_frame"/>
            <param name="videre_mode" type="str" value="none"/>
            <param name="convert_to_color" type="bool" value="True"/>
            
            <param name="exposure" type="int" value="450"/>
            <param name="exposure_auto" type="bool" value="False"/>
            <param name="brightness" type="int" value="50"/>
            <param name="brightness_auto" type="bool" value="False"/>
            <param name="gain" type="int" value="10"/>
            <param name="gain_auto" type="bool" value="True"/>
            
            <param name="hdr" type="bool" value="True"/>
            <!-- More available parameters ...
/stereo/videre_stereo_cam/brightness_max
/stereo/videre_stereo_cam/brightness_min
/stereo/videre_stereo_cam/companding
/stereo/videre_stereo_cam/convert_to_color
/stereo/videre_stereo_cam/exposure_max
/stereo/videre_stereo_cam/exposure_min
/stereo/videre_stereo_cam/fps
/stereo/videre_stereo_cam/frame_id
/stereo/videre_stereo_cam/gain_max
/stereo/videre_stereo_cam/gain_min
/stereo/videre_stereo_cam/hdr
/stereo/videre_stereo_cam/horopter
/stereo/videre_stereo_cam/params
/stereo/videre_stereo_cam/speckle_range
/stereo/videre_stereo_cam/speckle_size
/stereo/videre_stereo_cam/texture_threshold
/stereo/videre_stereo_cam/uniqueness_threshold
/stereo/videre_stereo_cam/videre_mode
/stereo/videre_stereo_cam/whitebalance_auto
/stereo/videre_stereo_cam/whitebalance_blue
/stereo/videre_stereo_cam/whitebalance_max
/stereo/videre_stereo_cam/whitebalance_min
/stereo/videre_stereo_cam/whitebalance_red
-->
        </node>
    </group>
</launch>

